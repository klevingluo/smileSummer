figure(1);
input = 'input.jpg';
example = 'example.jpg';

template = getLayers(warpFace(input, example));
face = getLayers(imread('input.jpg'));

imshow(template.lightness);

detail = 0.2* rgb2lab(template.detail) + 0.7 * rgb2lab(face.detail);
shape = 0.2* rgb2lab(template.shape) + 0.7 * rgb2lab(face.shape);

imshow(detail + shape + rgb2lab);